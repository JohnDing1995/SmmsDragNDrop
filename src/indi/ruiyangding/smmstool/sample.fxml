<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="indi.ruiyangding.smmstool.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <TabPane prefHeight="330.0" prefWidth="482.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Upload">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ProgressBar fx:id="uploadProgress" layoutX="21.0" layoutY="247.0" prefHeight="23.0" prefWidth="390.0" progress="0.0" />
                        <TableView fx:id="imagesTable" layoutX="14.0" layoutY="14.0" onDragDropped="#fileDropped" onDragOver="#fileDraggedOver" prefHeight="224.0" prefWidth="415.0">
                           <columns>
                              <TableColumn fx:id="name" prefWidth="75.0" text="File Name" />
                              <TableColumn fx:id="url" prefWidth="75.0" text="URL" />
                              <TableColumn fx:id="success" prefWidth="75.0" text="Success" />
                           </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="History">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab text="Delete">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</GridPane>
